    "contents": "<h1 id='header-%E5%B1%9E%E6%80%A7'>属性</h1><div class=\"k-table k-border\">\n                    <div class=\"k-table-wrapper\">\n                        <div class=\"k-thead\">\n                            <table>\n                                <thead><tr>\n<th>属性</th>\n<th>说明</th>\n<th>类型</th>\n<th>默认值</th>\n</tr>\n</thead>\n                            </table>\n                        </div>\n                        <div class=\"k-tbody\">\n                            <table>\n                                <tbody><tr>\n<td>type</td>\n<td>类型</td>\n<td><code>&quot;bar&quot;</code> &#124; <code>&quot;circle&quot;</code></td>\n<td><code>&quot;bar&quot;</code></td>\n</tr>\n<tr>\n<td>percent</td>\n<td>当前百分比(0~100)</td>\n<td><code>Number</code></td>\n<td><code>0</code></td>\n</tr>\n<tr>\n<td>size</td>\n<td>尺寸</td>\n<td><code>&quot;default&quot;</code> &#124; <code>&quot;small&quot;</code> &#124; <code>&quot;mini&quot;</code></td>\n<td><code>&quot;default&quot;</code></td>\n</tr>\n<tr>\n<td>isOuterText</td>\n<td>是否展示后面的百分比</td>\n<td><code>Boolean</code></td>\n<td><code>true</code></td>\n</tr>\n<tr>\n<td>isInnerText</td>\n<td>是否展示进度条上面的百分比</td>\n<td><code>Boolean</code></td>\n<td><code>false</code></td>\n</tr>\n<tr>\n<td>status</td>\n<td>进度条状态</td>\n<td><code>&quot;success&quot;</code> &#124; <code>&quot;error&quot;</code> &#124; <code>&quot;active&quot;</code> &#124; <code>&quot;normal&quot;</code> &#124; <code>&quot;warning&quot;</code></td>\n<td><code>&quot;active&quot;</code></td>\n</tr>\n<tr>\n<td>strokeWidth</td>\n<td>进度条指示线的宽度，单位<code>px</code>，只对<code>type=&quot;circle&quot;</code>圆形进度有效</td>\n<td><code>Number</code></td>\n<td><code>10</code></td>\n</tr>\n</tbody>\n                            </table>\n                        </div>\n                    </div>\n                </div>",
