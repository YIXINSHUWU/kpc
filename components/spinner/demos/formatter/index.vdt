import Spinner from 'kpc/components/spinner';

<div>
    <Spinner vertical 
        v-model="money"
        formatter={{ value => ('￥' + value).replace(/\B(?=(\d{3})+(?!\d))/g, ',') }}
        parser={{ value => value.replace(/￥|,/g, '') }}
    />
    <Spinner vertical 
        prefix="增长率 "
        suffix="%"
        v-model="percent"
    />
</div>