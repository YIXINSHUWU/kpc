import Upload from 'kpc/components/upload';
import Button from 'kpc/components/button';

<Upload 
    ref="instance"
    multiple 
    action="//jsonplaceholder.typicode.com/posts/"
    autoUpload={{ false }}
>
    <Button type="primary">选择文件</Button>
    <b:tip>
        <Button ev-click={{ self.upload }}>开始上传</Button>
    </b:tip>
</Upload>