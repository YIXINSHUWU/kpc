.example.index-132
    .k-col
        line-height 40px
        text-align center
        color #fff
    .no-gutter
        .k-col
            &:nth-of-type(even)
                background #0a5eee
            &:nth-of-type(odd)
                background lighten(#0a5eee, 30%)
    .gutter
        .k-col
            &:nth-of-type(even)
                div
                    background #0a5eee
            &:nth-of-type(odd)
                div
                    background lighten(#0a5eee, 30%)
    .k-row
        margin-bottom 20px